<h3>Documentation</h3>

<% if current_user %>
  <%= form_for documentations_url do |f| %>

    <%= f.hidden_field :user_id %>

    <div class="field">
      <%= f.label :text, "1. What is your name?" %><br />
      <%= f.text_field :text%>
    </div>

    <div class="field">
      <%= f.label :text, "2. Where are you located?" %><br />
      <%= f.text_field :text%>
    </div>

    <div class="actions"><%= submit_tag "Apply Now" %>
    </div>
  <% end %>
<% end %>
